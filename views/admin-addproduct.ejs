<!DOCTYPE html>
<html>
    <head>
        <link href="/css/style.css" rel="stylesheet">
        <title>Add product</title>
    </head>
    <body>
        <nav class="navigation-bar">
            <h3>Admin Page</h3>
        </nav>
        <form action="/admin/saveproduct" method="POST" class="form">
           
            <div>
                <label class="lbl">car name</label>
         <input type="text" name="carname" id="carname" class="inp" required>
            </div>
            <div>
                <label class="lbl">car imageurl  </label>
         <input type="text" name="carimageurl" id="carimageurl" class="inp" required>
            </div>
           <div>
                <label class="lbl">Price    </label>
         <input type="text" name="price" id="Price" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Model    </label>
         <input type="text" name="Model" id="Model" class="inp" required> 
            </div>
            <div>
            <label class="lbl">Brand    </label>
            <input type="text" name="Brand" id="Brand" class="inp" required> 
               </div>
            <div>
                <label class="lbl">Seats    </label>
         <input type="text" name="seats" id="seats" class="inp" required> 
            </div>
            <div>
                <label class="lbl">colors    </label>
         <input type="text" name="colors" id="colors" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Fuel    </label>
         <input type="text" name="fuel" id="fuel" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Launch_year    </label>
         <input type="text" name="launch_year" id="launch_year" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Category    </label>
         <input type="text" name="category" id="category" class="inp" required> 
            </div>
            <div>
                <label class="lbl">interior(img1)    </label>
         <input type="text" name="Iimg1" id="Iimg1" class="inp" required> 
            </div>
            <div>
                <label class="lbl">interior(img2)     </label>
         <input type="text" name="Iimg2" id="Iimg2" class="inp" required> 
            </div>
            <div>
                <label class="lbl">interior(img3)     </label>
         <input type="text" name="Iimg3" id="Iimg3" class="inp" required> 
            </div>
            <div>
                <label class="lbl">exterior(img1)     </label>
         <input type="text" name="Eimg1" id="Eimg1" class="inp" required> 
            </div>
            <div>
                <label class="lbl">exterior(img2)     </label>
         <input type="text" name="Eimg2" id="Eimg2" class="inp" required> 
            </div>
            <div>
                <label class="lbl">exterior(img3)    </label>
         <input type="text" name="Eimg3" id="Eimg3" class="inp" required> 
            </div>
            <input type="submit" value="SUBMIT" class="sub-btn">
            
        </form>
    
        <form action="/admin/addspecs" method="POST" class="specform">
            <div>
                <label class="lbl">car name</label>
         <input type="text" name="Scarname" id="carname" class="inp" required>
            </div>
            <div>
                <label class="lbl">Model  </label>
         <input type="text" name="Smodel" id="Model" class="inp" required>
            </div>
           <div>
                <label class="lbl">category   </label>
         <input type="text" name="Scategory" id="category" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Tyre size    </label>
         <input type="text" name="tyresize" id="tyresize" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Tank Capacity    </label>
         <input type="text" name="tankcapacity" id="tankcapacity" class="inp" required> 
            </div>
            <div>
                <label class="lbl"> Suspension   </label>
         <input type="text" name="suspension" id="suspension" class="inp" required> 
            </div>
            <div>
                <label class="lbl"> Ground clearance  </label>
         <input type="text" name="groundclearance" id="groundclearance" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Dimension    </label>
         <input type="text" name="Dimension" id="Dimension" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Transmission Type    </label>
         <input type="text" name="transmissiontype" id="transmissiontype" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Emission std </label>
         <input type="text" name="emissionstd" id="emissionstd" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Max Torque </label>
         <input type="text" name="maxtorque" id="maxtorque" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Max Power </label>
         <input type="text" name="maxpower" id="maxpower" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Engine displacement </label>
         <input type="text" name="Engdisplacement" id="Engdisplacement" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Bluetooth Audio </label>
         <input type="text" name="bluetoothaudio" id="bluetoothaudio" class="inp" required> 
            </div>
            <div>
                <label class="lbl">Gross Weight </label>
         <input type="text" name="grossweight" id="grossweight" class="inp" required> 
            </div>
            <input type="submit" value="SUBMIT" class="sub-btn">
            
        </form>
    </body>
</html>